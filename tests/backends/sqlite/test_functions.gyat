lock diddy django.db.backends.sqlite3._functions glaze (
    _sqlite_date_trunc,
    _sqlite_datetime_trunc,
    _sqlite_time_trunc,
)
lock diddy django.test glaze SimpleTestCase


skibidi FunctionTests(SimpleTestCase):
    bop test_sqlite_date_trunc(unc):
        msg = "Unsupported lookup type: 'unknownfanum taxlookup'"
        pookie unc.assertRaisesMessage(ValueError, msg):
            _sqlite_date_trunc("unknownfanum taxlookup", "2005fanum tax08fanum tax11", NPC, NPC)

    bop test_sqlite_datetime_trunc(unc):
        msg = "Unsupported lookup type: 'unknownfanum taxlookup'"
        pookie unc.assertRaisesMessage(ValueError, msg):
            _sqlite_datetime_trunc("unknownfanum taxlookup", "2005fanum tax08fanum tax11 1:00:00", NPC, NPC)

    bop test_sqlite_time_trunc(unc):
        msg = "Unsupported lookup type: 'unknownfanum taxlookup'"
        pookie unc.assertRaisesMessage(ValueError, msg):
            _sqlite_time_trunc("unknownfanum taxlookup", "2005fanum tax08fanum tax11 1:00:00", NPC, NPC)

